* {
	font-family: 微软雅黑;
}

body {
	background: #ECF0F5;
	margin: 0px;
	padding: 0px;
}

.top-button {
	margin-right: 10px;
	width: 100px;
}


/*主容器上部分*/

.content-top {
	margin-top: 20px;
	margin: 7px auto;
	margin-top: 32px;
	box-shadow: rgb(207, 203, 203) 0px 4px 10px;
	border-radius: 10px;
}


/**
 *弹出框样式 
 */

.tc-dialog {
	position: fixed;
	left: 0;
	top: 32px;
	right: 0;
	width: 600px;
	margin: 0px auto;
	text-align: center;
	display: none;
}

.modal-custom-header {
	height: 50px;
	color: #fff;
	background: #00A7D0;
	background-color: #00A7D0;
	display: block;
	padding: 0px 10px;
	position: relative;
	border-top-right-radius: 4px;
	border-top-left-radius: 4px;
}

.modal-custom-title {
	display: inline-block;
	font-size: 15px;
	margin: 0;
	line-height: 50px;
}


/*复选框css*/

.all-checkbox {
	vertical-align: middle;
	margin: auto;
	padding: 0;
	width: 20px;
	height: 20px;
}


/*表格标题居中*/

.all-table-title {
	vertical-align: middle;
	text-align: center;
}

.searchButton {
	color: #000;
	/* background: -webkit-linear-gradient(#00A9EC, #007FA9); */
	background: #fff;
	font-family: 微软雅黑;
	line-height: 13px;
	width: 100px;
}


/*条记录*/

.visble-border {
	height: 80px;
	text-align: center;
	padding-top: 22px;
	color: #337AB7;
	font-size: 18px;
	width: 150px;
}

.visble-border2 {
	height: 80px;
	text-align: center;
	padding-top: 22px;
	color: #337AB7;
	font-size: 18px;
	width: 250px;
}


/*表格距离设置*/

.table-distances {
	margin-top: 15px;
	margin-left: 15px;
	margin-right: 15px;
	width: 98%;
}

.common-title-bg {
	color: #fff;
	background: #00A7D0;
	background-color: #00A7D0;
}

.row-margintop {
	margin-top: 10px;
}

.row-button {
	margin: 0 auto;
	width: 300px;
	margin-top: 10px;
}

.row-width {
	width: 300px;
}